{
  "name": "@symptom-checker/ui-kit",
  "version": "0.0.1-beta.1",
  "private": true,
  "scripts": {
    "lint:mdx": "eslint --ext .mdx src --fix",
    "lint:js": "vue-cli-service lint --fix",
    "lint:scss": "stylelint 'src/**/*.{vue,htm,html,css,sss,less,scss}' --fix",
    "create-component": "node scripts/create-component.js",
    "create-index": "node scripts/create-index.js",
    "postinstall": "node scripts/create-index.js",
    "storybook:build": "node_modules/.bin/build-storybook -s ./public",
    "storybook:serve": "node_modules/.bin/start-storybook -s ./public -p 6006",
    "test:unit": "jest",
    "test:verbose": "jest --verbose"
  },
  "main": "index.js",
  "module": "index.js",
  "files": [
    "scripts/create-index.js",
    "src/*",
    "index.js"
  ],
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:js && npm run lint:scss"
    }
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
    "core-js": "^3.6.5",
    "uid": "^2.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@storybook/addon-a11y": "^6.2.0-beta.2",
    "@storybook/addon-actions": "^6.2.0-beta.2",
    "@storybook/addon-essentials": "^6.2.0-beta.2",
    "@storybook/addon-links": "^6.2.0-beta.2",
    "@storybook/addon-storyshots": "^6.2.0-beta.2",
    "@storybook/addon-viewport": "^6.2.0-beta.2",
    "@storybook/vue3": "^6.2.0-beta.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/test-utils": "^2.0.0-rc.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-mdx": "^1.9.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "file-save": "^0.2.0",
    "glob": "^7.1.6",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "prompts": "^2.4.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.0.5",
    "stylelint": "^13.11.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-scss": "^1.0.0-security",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "uppercamelcase": "^3.0.0",
    "vue-jest": "^5.0.0-alpha.8",
    "vue-loader": "^16.1.2"
  },
  "sideEffects": [
    "*.vue",
    "*.scss"
  ]
}
